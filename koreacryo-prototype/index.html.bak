<style>
@import url('https://fonts.googleapis.com/css2?family=Noto+Sans+KR:wght@300;400;500;600;700;900&display=swap');
body { font-family: 'Noto Sans KR', sans-serif; background: #060d1b; }
body.loading { overflow: hidden; }

/* ===== SCROLL PROGRESS BAR ===== */
#scroll-progress {
  position: fixed;
  top: 0;
  left: 0;
  height: 3px;
  width: 0%;
  background: linear-gradient(90deg, #00d4ff, #00e5ff, #00BFFF);
  z-index: 9999;
  box-shadow: 0 0 8px rgba(0, 212, 255, 0.6), 0 0 16px rgba(0, 212, 255, 0.3);
  transition: width 0.05s linear;
}

/* ===== LOADING INTRO ===== */
#loading-overlay {
  position: fixed;
  inset: 0;
  z-index: 10000;
  background: #060d1b;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  transition: transform 0.8s cubic-bezier(0.76, 0, 0.24, 1);
}
#loading-overlay.slide-up {
  transform: translateY(-100%);
}
#loading-overlay .kc-intro-logo {
  width: 120px;
  height: auto;
  animation: frost-crystallize 1.5s ease-out forwards;
  filter: drop-shadow(0 0 30px rgba(0, 212, 255, 0.8));
}
@keyframes frost-crystallize {
  0% { opacity: 0; transform: scale(0.5) rotate(-5deg); filter: blur(20px) drop-shadow(0 0 0px rgba(0,212,255,0)); }
  30% { opacity: 0.5; transform: scale(0.8) rotate(2deg); filter: blur(8px) drop-shadow(0 0 15px rgba(0,212,255,0.5)); }
  60% { opacity: 0.9; transform: scale(1.05) rotate(-1deg); filter: blur(2px) drop-shadow(0 0 25px rgba(0,212,255,0.7)); }
  100% { opacity: 1; transform: scale(1) rotate(0deg); filter: blur(0) drop-shadow(0 0 30px rgba(0,212,255,0.8)); }
}
#loading-temp {
  font-size: 2.5rem;
  font-weight: 900;
  margin-top: 24px;
  font-variant-numeric: tabular-nums;
  background: linear-gradient(135deg, #00e5ff, #00BFFF, #0080ff);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  text-shadow: none;
  filter: drop-shadow(0 0 10px rgba(0,212,255,0.5));
}
#loading-frost-particles {
  position: absolute;
  inset: 0;
  pointer-events: none;
  overflow: hidden;
}
.loading-frost-p {
  position: absolute;
  color: rgba(0, 229, 255, 0.3);
  animation: loading-frost-float 2s ease-out forwards;
}
@keyframes loading-frost-float {
  0% { opacity: 0; transform: translate(0, 0) scale(0); }
  30% { opacity: 1; }
  100% { opacity: 0; transform: translate(var(--dx), var(--dy)) scale(1); }
}

/* ===== CUSTOM CURSOR ===== */
#custom-cursor {
  position: fixed;
  width: 16px;
  height: 16px;
  border-radius: 50%;
  background: rgba(0, 212, 255, 0.8);
  box-shadow: 0 0 20px rgba(0, 212, 255, 0.6), 0 0 40px rgba(0, 212, 255, 0.3);
  pointer-events: none;
  z-index: 9998;
  transform: translate(-50%, -50%);
  transition: width 0.3s, height 0.3s, background 0.3s, box-shadow 0.3s;
  mix-blend-mode: screen;
  display: none;
}
#custom-cursor.hovering {
  width: 40px;
  height: 40px;
  background: rgba(0, 212, 255, 0.15);
  border: 2px solid rgba(0, 212, 255, 0.6);
  box-shadow: 0 0 30px rgba(0, 212, 255, 0.4), 0 0 60px rgba(0, 212, 255, 0.2);
}
#cursor-trail {
  position: fixed;
  width: 24px;
  height: 24px;
  border-radius: 50%;
  background: rgba(0, 212, 255, 0.15);
  box-shadow: 0 0 30px rgba(0, 212, 255, 0.2);
  pointer-events: none;
  z-index: 9997;
  transform: translate(-50%, -50%);
  transition: left 0.15s ease-out, top 0.15s ease-out;
  mix-blend-mode: screen;
  display: none;
}
@media (hover: none), (pointer: coarse) {
  #custom-cursor, #cursor-trail { display: none !important; }
}

/* ===== PRODUCT MODAL ===== */
#product-modal-overlay {
  position: fixed;
  inset: 0;
  z-index: 9000;
  background: rgba(6, 13, 27, 0.85);
  backdrop-filter: blur(12px);
  -webkit-backdrop-filter: blur(12px);
  display: none;
  align-items: center;
  justify-content: center;
  opacity: 0;
  transition: opacity 0.3s;
}
#product-modal-overlay.active {
  display: flex;
  opacity: 1;
}
#product-modal {
  background: linear-gradient(135deg, rgba(15, 34, 64, 0.9), rgba(6, 13, 27, 0.95));
  border: 1px solid rgba(0, 212, 255, 0.25);
  border-radius: 20px;
  padding: 32px;
  max-width: 560px;
  width: 90%;
  max-height: 80vh;
  overflow-y: auto;
  position: relative;
  transform: scale(0.85);
  transition: transform 0.35s cubic-bezier(0.16, 1, 0.3, 1);
  box-shadow: 0 0 60px rgba(0, 212, 255, 0.15), 0 25px 80px rgba(0, 0, 0, 0.5);
  backdrop-filter: blur(20px);
}
#product-modal-overlay.active #product-modal {
  transform: scale(1);
}
#modal-close {
  position: absolute;
  top: 16px;
  right: 16px;
  width: 36px;
  height: 36px;
  border-radius: 50%;
  border: 1px solid rgba(0, 212, 255, 0.3);
  background: rgba(0, 212, 255, 0.1);
  color: #80f0ff;
  font-size: 18px;
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  transition: all 0.2s;
}
#modal-close:hover {
  background: rgba(0, 212, 255, 0.2);
  border-color: rgba(0, 212, 255, 0.5);
}

/* ===== MOBILE MENU ANIMATION ===== */
#menu-btn .hamburger-line {
  display: block;
  width: 24px;
  height: 2px;
  background: white;
  transition: all 0.3s cubic-bezier(0.16, 1, 0.3, 1);
  transform-origin: center;
}
#menu-btn.active .hamburger-line:nth-child(1) {
  transform: translateY(8px) rotate(45deg);
}
#menu-btn.active .hamburger-line:nth-child(2) {
  opacity: 0;
  transform: scaleX(0);
}
#menu-btn.active .hamburger-line:nth-child(3) {
  transform: translateY(-8px) rotate(-45deg);
}
#mobile-menu {
  max-height: 0;
  overflow: hidden;
  transition: max-height 0.4s cubic-bezier(0.16, 1, 0.3, 1), opacity 0.3s;
  opacity: 0;
}
#mobile-menu.open {
  max-height: 400px;
  opacity: 1;
}
#mobile-menu .mobile-link {
  opacity: 0;
  transform: translateY(-10px);
  transition: opacity 0.3s, transform 0.3s;
}
#mobile-menu.open .mobile-link {
  opacity: 1;
  transform: translateY(0);
}
#mobile-menu.open .mobile-link:nth-child(1) { transition-delay: 0.05s; }
#mobile-menu.open .mobile-link:nth-child(2) { transition-delay: 0.1s; }
#mobile-menu.open .mobile-link:nth-child(3) { transition-delay: 0.15s; }
#mobile-menu.open .mobile-link:nth-child(4) { transition-delay: 0.2s; }
#mobile-menu.open .mobile-link:nth-child(5) { transition-delay: 0.25s; }
#mobile-menu.open .mobile-link:nth-child(6) { transition-delay: 0.3s; }
#mobile-menu.open .mobile-link:nth-child(7) { transition-delay: 0.35s; }

/* ===== PARTICLES ===== */
.particle {
  position: absolute;
  background: rgba(0, 212, 255, 0.4);
  border-radius: 50%;
  animation: float linear infinite;
  box-shadow: 0 0 6px rgba(0, 212, 255, 0.3);
}
@keyframes float {
  0% { transform: translateY(0) scale(1); opacity: 0; }
  10% { opacity: 1; }
  90% { opacity: 0.6; }
  100% { transform: translateY(-100vh) scale(0.3); opacity: 0; }
}

/* ===== ICE CRYSTALS ===== */
.ice-crystal {
  position: absolute;
  color: rgba(0, 229, 255, 0.15);
  font-size: 18px;
  animation: crystal-fall linear infinite;
  pointer-events: none;
}
@keyframes crystal-fall {
  0% { transform: translateY(-20px) rotate(0deg); opacity: 0; }
  10% { opacity: 1; }
  90% { opacity: 0.3; }
  100% { transform: translateY(100vh) rotate(360deg); opacity: 0; }
}

/* ===== FROST LINES ===== */
@keyframes frost-sweep {
  0% { transform: translateX(-100%); opacity: 0; }
  20% { opacity: 1; }
  80% { opacity: 1; }
  100% { transform: translateX(100vw); opacity: 0; }
}
.frost-line {
  position: absolute;
  height: 2px;
  width: 300px;
  background: linear-gradient(90deg, transparent, rgba(0,212,255,0.8), rgba(0,229,255,0.4), transparent);
  animation: frost-sweep 3s linear infinite;
  filter: blur(0.5px);
  box-shadow: 0 0 8px rgba(0,212,255,0.3);
}

/* ===== GLITCH TEXT ===== */
.glitch {
  position: relative;
  display: inline-block;
}
.glitch::before, .glitch::after {
  content: attr(data-text);
  position: absolute;
  top: 0; left: 0;
  width: 100%; height: 100%;
}
.glitch::before {
  color: #00e5ff;
  animation: glitch-1 3s infinite linear alternate-reverse;
  clip-path: polygon(0 0, 100% 0, 100% 33%, 0 33%);
}
.glitch::after {
  color: #0080ff;
  animation: glitch-2 3s infinite linear alternate-reverse;
  clip-path: polygon(0 67%, 100% 67%, 100% 100%, 0 100%);
}
@keyframes glitch-1 {
  0%, 95% { transform: none; opacity: 0; }
  96% { transform: translate(2px, -2px); opacity: 0.8; }
  97% { transform: translate(-2px, 2px); opacity: 0; }
  98% { transform: translate(2px, 1px); opacity: 0.8; }
  99% { transform: translate(-1px, -1px); opacity: 0; }
  100% { transform: none; opacity: 0; }
}
@keyframes glitch-2 {
  0%, 93% { transform: none; opacity: 0; }
  94% { transform: translate(-2px, 2px); opacity: 0.8; }
  95% { transform: translate(2px, -1px); opacity: 0; }
  96% { transform: translate(-1px, 2px); opacity: 0.8; }
  97% { transform: translate(1px, -2px); opacity: 0; }
  100% { transform: none; opacity: 0; }
}

/* ===== GRADIENT TEXT ===== */
.gradient-text {
  background: linear-gradient(135deg, #00e5ff, #00BFFF, #0080ff, #00e5ff);
  background-size: 200% 200%;
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  animation: gradient-shift 4s ease infinite;
}
@keyframes gradient-shift {
  0%, 100% { background-position: 0% 50%; }
  50% { background-position: 100% 50%; }
}

/* ===== SCROLL REVEAL ===== */
.reveal {
  opacity: 0;
  transform: translateY(40px);
  transition: all 0.8s cubic-bezier(0.16, 1, 0.3, 1);
}
.reveal.visible {
  opacity: 1;
  transform: translateY(0);
}

/* ===== CARD HOVER WITH GLOW ===== */
.product-card {
  transition: all 0.5s cubic-bezier(0.16, 1, 0.3, 1);
  position: relative;
  overflow: hidden;
}
.product-card::before {
  content: '';
  position: absolute;
  inset: -2px;
  background: linear-gradient(135deg, transparent, rgba(0,212,255,0.1), transparent);
  border-radius: inherit;
  opacity: 0;
  transition: opacity 0.5s;
}
.product-card:hover::before { opacity: 1; }
.product-card:hover {
  transform: translateY(-10px);
  box-shadow: 0 0 40px rgba(0, 212, 255, 0.15), 0 20px 60px rgba(0, 191, 255, 0.1);
  border-color: rgba(0, 212, 255, 0.4);
}

/* ===== NAV BLUR ===== */
.nav-blur {
  backdrop-filter: blur(24px);
  -webkit-backdrop-filter: blur(24px);
}

/* ===== HERO ENTRANCE ===== */
@keyframes hero-in {
  0% { opacity: 0; transform: translateY(30px) scale(0.95); filter: blur(10px); }
  100% { opacity: 1; transform: translateY(0) scale(1); filter: blur(0); }
}
.hero-anim { animation: hero-in 1s cubic-bezier(0.16, 1, 0.3, 1) forwards; }
.hero-anim-d1 { animation-delay: 0.2s; opacity: 0; }
.hero-anim-d2 { animation-delay: 0.5s; opacity: 0; }
.hero-anim-d3 { animation-delay: 0.8s; opacity: 0; }

/* ===== MARQUEE ===== */
@keyframes marquee {
  0% { transform: translateX(0); }
  100% { transform: translateX(-50%); }
}
.marquee-track { animation: marquee 20s linear infinite; }
.marquee-track:hover { animation-play-state: paused; }

/* ===== PULSE RING ===== */
@keyframes pulse-ring {
  0% { transform: scale(1); opacity: 0.4; }
  100% { transform: scale(1.8); opacity: 0; }
}

/* ===== TEMPERATURE GAUGE ===== */
.temp-gauge {
  position: fixed;
  right: 20px;
  top: 50%;
  transform: translateY(-50%);
  z-index: 40;
  transition: opacity 0.5s;
  pointer-events: none;
}
.temp-milestones {
  position: absolute;
  right: 60px;
  top: 50%;
  transform: translateY(-50%);
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  gap: 0;
}
.temp-milestone {
  white-space: nowrap;
  background: rgba(6, 13, 27, 0.9);
  border: 1px solid rgba(0, 212, 255, 0.3);
  border-radius: 8px;
  padding: 5px 12px;
  font-size: 11px;
  font-weight: 600;
  color: #b3f5ff;
  opacity: 0;
  transform: translateX(10px);
  transition: opacity 0.4s ease, transform 0.4s ease;
  backdrop-filter: blur(8px);
  box-shadow: 0 0 16px rgba(0, 212, 255, 0.15);
  pointer-events: none;
  position: absolute;
  right: 0;
}
.temp-milestone.active {
  opacity: 1;
  transform: translateX(0);
}
@media (max-width: 768px) {
  .temp-milestones { display: none; }
  .temp-gauge { right: 10px; }
}
.temp-bar {
  width: 4px;
  height: 140px;
  background: rgba(255,255,255,0.1);
  border-radius: 2px;
  position: relative;
  overflow: hidden;
}
.temp-fill {
  position: absolute;
  bottom: 0;
  width: 100%;
  background: linear-gradient(to top, #00e5ff, #0050ff);
  border-radius: 2px;
  transition: height 0.1s;
  box-shadow: 0 0 10px rgba(0,212,255,0.5);
}
.temp-value {
  font-size: 14px;
  font-weight: 900;
  color: #00e5ff;
  text-align: center;
  margin-top: 6px;
  text-shadow: 0 0 10px rgba(0,212,255,0.5);
  font-variant-numeric: tabular-nums;
}

/* ===== PARALLAX HELPERS ===== */
.parallax-slow { transition: transform 0.1s linear; }

/* ===== SCANLINE OVERLAY ===== */
.scanlines {
  position: absolute;
  inset: 0;
  background: repeating-linear-gradient(
    0deg,
    transparent,
    transparent 2px,
    rgba(0, 212, 255, 0.015) 2px,
    rgba(0, 212, 255, 0.015) 4px
  );
  pointer-events: none;
  z-index: 1;
}

/* ===== LOGO IMAGE ===== */
.kc-logo { height: 40px; width: auto; }
.kc-logo-sm { height: 32px; width: auto; }
.kc-logo-footer { height: 44px; width: auto; }

/* ===== TIMELINE ===== */
.timeline-container { position: relative; }
.timeline-line {
  position: absolute;
  left: 50%;
  top: 0;
  bottom: 0;
  width: 2px;
  background: linear-gradient(to bottom, transparent, rgba(0,212,255,0.4), rgba(0,212,255,0.4), transparent);
  transform: translateX(-50%);
}
@media (max-width: 768px) {
  .timeline-line { left: 20px; }
}
.timeline-dot {
  width: 16px;
  height: 16px;
  border-radius: 50%;
  background: #00d4ff;
  box-shadow: 0 0 20px rgba(0,212,255,0.6), 0 0 40px rgba(0,212,255,0.3);
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  z-index: 2;
}
@media (max-width: 768px) {
  .timeline-dot { left: 20px; }
}
.timeline-item {
  position: relative;
  width: 50%;
  padding: 0 40px 40px;
}
.timeline-item:nth-child(odd) {
  left: 0;
  text-align: right;
}
.timeline-item:nth-child(even) {
  left: 50%;
  text-align: left;
}
@media (max-width: 768px) {
  .timeline-item,
  .timeline-item:nth-child(odd),
  .timeline-item:nth-child(even) {
    width: 100%;
    left: 0;
    text-align: left;
    padding-left: 50px;
    padding-right: 16px;
  }
}
.timeline-year {
  font-size: 1.5rem;
  font-weight: 900;
  background: linear-gradient(135deg, #00e5ff, #00BFFF);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}
.timeline-text {
  color: rgba(128, 240, 255, 0.8);
  font-size: 0.9rem;
  line-height: 1.6;
}
.timeline-highlight {
  color: #00e5ff;
  font-weight: 700;
}

/* ===== H2 GLOW ===== */
@keyframes h2-glow-pulse {
  0%, 100% { filter: drop-shadow(0 0 8px rgba(0,212,255,0.3)); }
  50% { filter: drop-shadow(0 0 20px rgba(0,212,255,0.5)); }
}

/* ===== LANG TOGGLE ===== */
#lang-toggle {
  cursor: pointer;
  font-size: 18px;
  transition: transform 0.3s;
  user-select: none;
}
#lang-toggle:hover { transform: scale(1.15); }
</style>
</head>
