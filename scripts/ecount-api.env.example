# eCount API Configuration Template
# Copy this file to .env and fill in your credentials

# eCount Company Code (6 digits)
ECOUNT_COM_CODE=123456

# eCount User ID
ECOUNT_USER_ID=admin

# API Certification Key (from eCount portal)
ECOUNT_API_KEY=your_api_cert_key_here

# Zone code (usually CC or KR)
ECOUNT_ZONE=CC

# Language (ko-KR, en-US, zh-CN)
ECOUNT_LAN_TYPE=ko-KR
